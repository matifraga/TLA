          { return sf.newSymbol("Variable name", sym.VAR_NAME, new String(yytext()));
          }
        case 62: break;
        case 6: 
          { return sf.newSymbol("Minus", sym.MINUS);
          }
        case 63: break;
        case 5: 
          { return sf.newSymbol("Plus", sym.PLUS);
          }
        case 64: break;
        case 32: 
          { return sf.newSymbol("Else", sym.ELSE);
          }
        case 65: break;
        case 2: 
          { return sf.newSymbol("Integral Number", sym.NUMBER, new Integer(yytext()));
          }
        case 66: break;
        case 10: 
          { return sf.newSymbol("Left Bracket", sym.LPAREN);
          }
        case 67: break;
        case 16: 
          { return sf.newSymbol("Text", sym.TEXT, new String(yytext()));
          }
        case 68: break;
        case 13: 
          { return sf.newSymbol("Greater", sym.CMP_GT);
          }
        case 69: break;
        case 1: 
          { System.err.println("Illegal character: "+yytext());
          }
        case 70: break;
        case 22: 
          { return sf.newSymbol("Assign Divide", sym.AS_DIV);
          }
        case 71: break;
        case 31: 
          { return sf.newSymbol("And", sym.BOOL_AND);
          }
        case 72: break;
        case 36: 
          { return sf.newSymbol("Begin", sym.BEGIN);
          }
        case 73: break;
        case 25: 
          { return sf.newSymbol("In", sym.IO_IN);
          }
        case 74: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            zzDoEOF();
              {     return sf.newSymbol("EOF", sym.EOF);
 }
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
